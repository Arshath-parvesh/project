<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFAFA"
    tools:context="com.example.project.gpsparking.Setuserdetails"

    >

    <EditText
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="monospace"
        android:textSize="22sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText,RtlHardcoded"
        android:hint="Radius(m)"
        android:inputType="text"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/editText4"
        android:layout_alignLeft="@+id/editText4"
        android:layout_alignStart="@+id/editText4"
        android:layout_alignRight="@+id/editText4"
        android:layout_alignEnd="@+id/editText4" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="27dp"
        android:fontFamily="monospace"
        android:textSize="24sp"
        android:textStyle="italic"
        tools:ignore="HardcodedText,UnknownIdInLayout"

        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Set Parking Location"
        tools:textStyle="bold" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="monospace"
        android:textSize="22sp"
        android:textStyle="bold|italic"
        tools:ignore="HardcodedText,RtlHardcoded"
        android:hint="Latitude"
        android:inputType="text"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/button1"
        android:layout_alignLeft="@+id/editText3"
        android:layout_alignStart="@+id/editText3"
        tools:textStyle="bold"
        android:layout_alignRight="@+id/button1"
        android:layout_alignEnd="@+id/button1" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="monospace"
        android:textSize="22sp"
        android:textStyle="italic"
        tools:ignore="HardcodedText,RtlHardcoded"
        android:hint="Longitude"
        android:inputType="text"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/editText"
        android:layout_alignLeft="@+id/button1"
        android:layout_alignStart="@+id/button1"
        tools:textStyle="bold"
        android:layout_alignRight="@+id/button1"
        android:layout_alignEnd="@+id/button1" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:background="@color/colorPrimary"
        android:textColor="#d2f"

        android:text="live location"
        android:textStyle="italic"
        tools:ignore="HardcodedText"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"


        android:background="@color/colorPrimary"
        android:textColor="#d2f"
        android:text="Upload Parking Details"
        android:textStyle="italic"
        tools:ignore="HardcodedText"
        android:layout_marginBottom="24dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/editText3"
        android:layout_alignStart="@+id/editText3"
        android:layout_alignRight="@+id/editText3"
        android:layout_alignEnd="@+id/editText3" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Place Details"
        android:fontFamily="monospace"
        android:textSize="22sp"
        android:textStyle="italic"

        android:id="@+id/editText6"
        android:inputType="text"
        android:layout_below="@+id/editText3"
        android:layout_alignLeft="@+id/editText3"
        android:layout_alignStart="@+id/editText3"
        android:layout_marginTop="51dp"
        android:layout_alignRight="@+id/button"
        android:layout_alignEnd="@+id/button" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:fontFamily="monospace"
        android:textSize="22sp"
        tools:ignore="HardcodedText,RtlHardcoded"
        android:hint="Next Parking Zone "
        android:inputType="text"
        android:layout_marginTop="34dp"
        android:textStyle="bold|italic"
        tools:textStyle="bold"
        android:layout_below="@+id/editText5"
        android:layout_alignLeft="@+id/editText5"
        android:layout_alignStart="@+id/editText5"
        android:layout_alignRight="@+id/editText5"
        android:layout_alignEnd="@+id/editText5" />

</RelativeLayout>
